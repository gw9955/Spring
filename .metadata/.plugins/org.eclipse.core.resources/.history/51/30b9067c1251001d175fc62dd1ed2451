	이를 구별하기 위한 식별 용도로 사용
 -->
 <mapper namespace="book">
 	<!-- MyBatis에서 제공해주는 데이터 입력을 나타내는 태그
 		1) 들어오는것 : Dao 객체가 던진 데이터타입(String, int, hashMap, VO, List)
 					 parameterType만 쓴다
 	 -->
 	<insert id="insert" parameterType="vo.BookVO">
 		
 	</insert>
 </mapper>